<GameInfo>
	<!-- Reducing yields from feature removal to offset the huge builder charge count. -->
	<Feature_Removes>
		<Update>
			<Where FeatureType="FEATURE_FOREST" YieldType="YIELD_PRODUCTION"/>
			<Set Yield="5"/>
		</Update>
		<Update>
			<Where FeatureType="FEATURE_JUNGLE" YieldType="YIELD_PRODUCTION"/>
			<Set Yield="2"/>
		</Update>
		<Update>
			<Where FeatureType="FEATURE_JUNGLE" YieldType="YIELD_FOOD"/>
			<Set Yield="2"/>
		</Update>
		<Update>
			<Where FeatureType="FEATURE_MARSH" YieldType="YIELD_FOOD"/>
			<Set Yield="5"/>
		</Update>
	</Feature_Removes>
</GameInfo>