#################################################################
# Object Properties (БЕЗ ИЗМЕНЕНИЙ)
#################################################################

wow:hasSpecialization a owl:ObjectProperty ;
 rdfs:domain wow:PlayerClass ;
 rdfs:range wow:Specialization ;
 rdfs:label "has specialization"@en , "имеет специализацию"@ru ;
 owl:inverseOf wow:ofClass .

wow:ofClass a owl:ObjectProperty ;
 rdfs:domain wow:Specialization ;
 rdfs:range wow:PlayerClass ;
 rdfs:label "of class"@en , "класса"@ru .

wow:hasRole a owl:ObjectProperty ;
 rdfs:domain wow:Specialization ;
 rdfs:range wow:Role ;
 rdfs:label "has role"@en , "имеет роль"@ru .

wow:suitableFor a owl:ObjectProperty ;
 rdfs:domain wow:Item ;
 rdfs:range wow:Specialization ;
 rdfs:label "suitable for"@en , "подходит для спеки"@ru .

wow:hasSlotObj a owl:ObjectProperty ;
 rdfs:domain wow:Item ;
 rdfs:range wow:Slot ;
 rdfs:label "has slot"@en , "слот"@ru .

wow:hasArmorType a owl:ObjectProperty ;
 rdfs:domain wow:Armor ;
 rdfs:range wow:ArmorType ;
 rdfs:label "has armor type"@en , "тип брони"@ru .

wow:dropsFrom a owl:ObjectProperty ;
 rdfs:domain wow:Item ;
 rdfs:range wow:Source ;
 rdfs:label "drops from"@en , "добывается у"@ru .

wow:locatedIn a owl:ObjectProperty ;
 rdfs:domain wow:Source ;
 rdfs:range wow:Activity ;
 rdfs:label "located in"@en , "находится в"@ru .

wow:hasEffect a owl:ObjectProperty ;
 rdfs:domain wow:Item ;
 rdfs:range wow:Effect ;
 rdfs:label "has effect"@en , "содержит эффект"@ru .

wow:effectiveIn a owl:ObjectProperty ;
 rdfs:domain wow:Effect ;
 rdfs:range wow:Activity ;
 rdfs:label "effective in"@en , "эффективен в"@ru .

wow:synergizesWith a owl:ObjectProperty ;
 rdfs:domain wow:Trinket ;
 rdfs:range wow:Specialization ;
 rdfs:label "synergizes with"@en , "синергирует с"@ru .

wow:conflictsWith a owl:ObjectProperty , owl:SymmetricProperty ;
 rdfs:domain wow:Item ;
 rdfs:range wow:Item ;
 rdfs:label "conflicts with"@en , "конфликтует с"@ru .

wow:alternativeTo a owl:ObjectProperty , owl:SymmetricProperty ;
 rdfs:domain wow:Item ;
 rdfs:range wow:Item ;
 rdfs:label "alternative to"@en , "альтернатива"@ru .

wow:usesEnchant a owl:ObjectProperty ;
 rdfs:domain wow:Item ;
 rdfs:range wow:Enchant ;
 rdfs:label "uses enchant"@en , "использует чары"@ru .

wow:usesGem a owl:ObjectProperty ;
 rdfs:domain wow:Item ;
 rdfs:range wow:Gem ;
 rdfs:label "uses gem"@en , "использует самоцвет"@ru .

wow:providesStat a owl:ObjectProperty ;
 rdfs:domain wow:Item ;
 rdfs:range wow:Stat ;
 rdfs:label "provides stat"@en , "даёт стат"@ru .

wow:requiresClass a owl:ObjectProperty ;
 rdfs:domain wow:Item ;
 rdfs:range wow:PlayerClass ;
 rdfs:label "requires class"@en , "требует класс"@ru .

wow:hasDifficulty a owl:ObjectProperty ;
 rdfs:domain wow:Activity ;
 rdfs:range wow:Difficulty ;
 rdfs:label "has difficulty"@en , "сложность"@ru .

wow:inSeason a owl:ObjectProperty ;
 rdfs:domain wow:Item ;
 rdfs:range wow:Season ;
 rdfs:label "in season/patch"@en , "в сезоне/патче"@ru .

#################################################################
