  {
    "kind": 2,
    "language": "sparql",
    "value": "PREFIX ns: <http://www.semanticweb.org/gangster/ontologies/2025/9/accessorizer-3000#>\nPREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>\n\nSELECT ?accessory ?rarity ?intelligence ?family ?requiresLevel\nWHERE {\n    {\n        SELECT ?accessory ?rarity ?intelligence ?family ?requiresLevel ?weight\n        WHERE {\n            ?accessory rdf:type ns:Accessory \n                ; ns:hasRarity ?rarity \n                ; ns:hasIntelligence ?intelligence\n                ; ns:requiresLevel ?requiresLevel.\n            \n            ?rarity ns:rarityWeight ?weight .\n            \n            OPTIONAL { ?accessory ns:belongsToFamily ?family . }\n            OPTIONAL { ?accessory ns:requiresLevel ?requiresLevel . }\n            OPTIONAL { ?accessory ns:requiresMiningSkill ?requiresMining . }\n            OPTIONAL { ?accessory ns:requiresFarmingSkill ?requiresFarming . }\n            OPTIONAL { ?accessory ns:requiresFishingSkill ?requiresFishing . }\n            OPTIONAL { ?accessory ns:requiresCombatSkill ?requiresCombat . }\n\n            FILTER (\n              (BOUND(?requiresLevel) && ?requiresLevel <= 39)\n              || (BOUND(?requiresMining) && ?requiresMining <= 10)\n              || (BOUND(?requiresFarming) && ?requiresFarming <= 10)\n              || (BOUND(?requiresFishing) && ?requiresFishing <= 10)\n              || (BOUND(?requiresCombat) && ?requiresCombat <= 9)\n            )\n            FILTER (!BOUND(?family))\n        }\n        ORDER BY DESC(?intelligence)\n    }\n    \n    UNION\n    \n    {\n        SELECT ?accessory ?rarity ?intelligence ?family ?requiresLevel\n        WHERE {\n            {\n                SELECT ?family (MAX(?w) as ?maxWeight)\n                WHERE {\n                    ?acc rdf:type ns:Accessory \n                        ; ns:belongsToFamily ?family\n                        ; ns:hasRarity ?r .\n                    ?r ns:rarityWeight ?w .\n                    \n                    OPTIONAL { ?acc ns:requiresLevel ?lvl . }\n                    OPTIONAL { ?acc ns:requiresMiningSkill ?mining . }\n                    OPTIONAL { ?acc ns:requiresFarmingSkill ?farming . }\n                    OPTIONAL { ?acc ns:requiresFishingSkill ?fishing . }\n                    OPTIONAL { ?acc ns:requiresCombatSkill ?combat . }\n\n                    FILTER (\n                      (BOUND(?lvl) && ?lvl <= 48)\n                      || (BOUND(?mining) && ?mining <= 12)\n                      || (BOUND(?farming) && ?farming <= 12)\n                      || (BOUND(?fishing) && ?fishing <= 12)\n                      || (BOUND(?combat) && ?combat <= 12)\n                    )\n                }\n                GROUP BY ?family\n            }\n            \n            ?accessory rdf:type ns:Accessory \n                ; ns:belongsToFamily ?family\n                ; ns:hasRarity ?rarity \n                ; ns:hasIntelligence ?intelligence\n                ; ns:requiresLevel ?requiresLevel.\n            \n            ?rarity ns:rarityWeight ?weight .\n            \n            FILTER (?weight = ?maxWeight)\n            \n            OPTIONAL { ?accessory ns:requiresLevel ?requiresLevel . }\n            OPTIONAL { ?accessory ns:requiresMiningSkill ?requiresMining . }\n            OPTIONAL { ?accessory ns:requiresFarmingSkill ?requiresFarming . }\n            OPTIONAL { ?accessory ns:requiresFishingSkill ?requiresFishing . }\n            OPTIONAL { ?accessory ns:requiresCombatSkill ?requiresCombat . }\n\n            FILTER (\n              (BOUND(?requiresLevel) && ?requiresLevel <= 48)\n              || (BOUND(?requiresMining) && ?requiresMining <= 12)\n              || (BOUND(?requiresFarming) && ?requiresFarming <= 12)\n              || (BOUND(?requiresFishing) && ?requiresFishing <= 12)\n              || (BOUND(?requiresCombat) && ?requiresCombat <= 12)\n            )\n        }\n        ORDER BY DESC(?intelligence)\n    }\n}\nORDER BY DESC(?intelligence)\nLIMIT 100",
    "metadata": {}
  },
