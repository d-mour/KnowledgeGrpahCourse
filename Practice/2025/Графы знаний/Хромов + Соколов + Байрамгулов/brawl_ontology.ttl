@prefix : <http://example.org/brawlstars#> .
@prefix bs: <http://example.org/brawlstars#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://example.org/brawlstars>
    a owl:Ontology ;
    rdfs:label "Brawl Stars Ontology"@en ;
    rdfs:comment "Компонентная онтология для игры Brawl Stars: бойцы, редкости, классы, режимы, карты, способности, гаджеты, стар-пауэры, передачи и семантические связи."@ru ;
    owl:versionInfo "1.0.0" ;
    rdfs:seeAlso <https://brawlify.com/brawlers/>, <https://support.supercell.com/brawl-stars/en/articles/game-modes-10.html> .

bs:Entity a owl:Class ; rdfs:label "Entity"@en .

bs:Brawler a owl:Class ;
    rdfs:subClassOf bs:Entity ;
    rdfs:label "Brawler"@en ;
    rdfs:comment "Игровой персонаж — основной юнит."@ru .

bs:Rarity a owl:Class ;
    rdfs:subClassOf bs:Entity ;
    rdfs:label "Rarity"@en ;
    owl:oneOf ( bs:Common bs:Rare bs:SuperRare bs:Epic bs:Mythic bs:Legendary bs:Starting ) .

bs:ClassRole a owl:Class ;
    rdfs:subClassOf bs:Entity ;
    rdfs:label "Class/Role"@en ;
    owl:oneOf ( bs:DamageDealer bs:Assassin bs:Controller bs:Marksman bs:Tank bs:Artillery bs:Support ) .

bs:Ability a owl:Class ; rdfs:subClassOf bs:Entity ; rdfs:label "Ability"@en .
bs:MainAttack a owl:Class ; rdfs:subClassOf bs:Ability ; rdfs:label "Main Attack"@en .
bs:Super a owl:Class ; rdfs:subClassOf bs:Ability ; rdfs:label "Super"@en .
bs:Gadget a owl:Class ; rdfs:subClassOf bs:Ability ; rdfs:label "Gadget"@en .
bs:StarPower a owl:Class ; rdfs:subClassOf bs:Ability ; rdfs:label "Star Power"@en .
bs:Gear a owl:Class ; rdfs:subClassOf bs:Entity ; rdfs:label "Gear"@en .
bs:Trait a owl:Class ; rdfs:subClassOf bs:Entity ; rdfs:label "Trait"@en .
bs:GameMode a owl:Class ; rdfs:subClassOf bs:Entity ; rdfs:label "Game Mode"@en .
bs:Map a owl:Class ; rdfs:subClassOf bs:Entity ; rdfs:label "Map"@en ; rdfs:comment "Конкретная карта, привязанная к режиму."@ru .
bs:TeamComp a owl:Class ; rdfs:subClassOf bs:Entity ; rdfs:label "Team Composition"@en .

bs:hasRarity a owl:ObjectProperty ; rdfs:domain bs:Brawler ; rdfs:range bs:Rarity ; rdfs:label "has rarity"@en .
bs:hasClass a owl:ObjectProperty ; rdfs:domain bs:Brawler ; rdfs:range bs:ClassRole ; rdfs:label "has class/role"@en .
bs:hasAttack a owl:ObjectProperty ; rdfs:domain bs:Brawler ; rdfs:range bs:MainAttack ; rdfs:label "has main attack"@en .
bs:hasSuper a owl:ObjectProperty ; rdfs:domain bs:Brawler ; rdfs:range bs:Super ; rdfs:label "has super"@en .
bs:hasGadget a owl:ObjectProperty ; rdfs:domain bs:Brawler ; rdfs:range bs:Gadget ; rdfs:label "has gadget"@en .
bs:hasStarPower a owl:ObjectProperty ; rdfs:domain bs:Brawler ; rdfs:range bs:StarPower ; rdfs:label "has star power"@en .
bs:hasGear a owl:ObjectProperty ; rdfs:domain bs:Brawler ; rdfs:range bs:Gear ; rdfs:label "has gear"@en .
bs:suitedForMode a owl:ObjectProperty ; rdfs:domain bs:Brawler ; rdfs:range bs:GameMode ; rdfs:label "suited for mode"@en .
bs:recommendedOnMap a owl:ObjectProperty ; rdfs:domain bs:Brawler ; rdfs:range bs:Map ; rdfs:label "recommended on map"@en .
bs:playedInMode a owl:ObjectProperty ; rdfs:domain bs:Map ; rdfs:range bs:GameMode ; rdfs:label "map played in mode"@en .
bs:counters a owl:ObjectProperty ; rdfs:domain bs:Brawler ; rdfs:range bs:Brawler ; rdfs:label "counters"@en ; owl:propertyDisjointWith bs:synergizesWith .
bs:synergizesWith a owl:ObjectProperty ; rdfs:domain bs:Brawler ; rdfs:range bs:Brawler ; rdfs:label "synergizes with"@en .
bs:memberOf a owl:ObjectProperty ; rdfs:domain bs:Brawler ; rdfs:range bs:TeamComp ; rdfs:label "member of team composition"@en .

bs:hasHealth a owl:DatatypeProperty ; rdfs:domain bs:Brawler ; rdfs:range xsd:integer ; rdfs:label "base health"@en .
bs:hasRange a owl:DatatypeProperty ; rdfs:domain bs:MainAttack ; rdfs:range xsd:string ; rdfs:label "range (qualitative)"@en .
bs:hasReloadSpeed a owl:DatatypeProperty ; rdfs:domain bs:MainAttack ; rdfs:range xsd:string ; rdfs:label "reload speed (qualitative)"@en .
bs:movementSpeed a owl:DatatypeProperty ; rdfs:domain bs:Brawler ; rdfs:range xsd:string ; rdfs:label "movement speed (qualitative)"@en .
bs:description a owl:DatatypeProperty ; rdfs:domain bs:Entity ; rdfs:range xsd:string ; rdfs:label "description"@en .

