  <!-- === Пример рецепта === -->
  <!-- Воздух + Воздух = Ветер -->
  <rdf:Description rdf:about="#Recipe_Воздух_Воздух">
    <rdf:type rdf:resource="#Recipe"/>
    <ex:hasIngredient rdf:resource="#Воздух"/>
    <ex:hasIngredient rdf:resource="#Воздух"/>
    <ex:hasResult rdf:resource="#Ветер"/>
    <rdfs:label xml:lang="ru">Воздух + Воздух = Ветер</rdfs:label>
  </rdf:Description>

  <!-- Воздух + Земля = Пыль -->
  <rdf:Description rdf:about="#Recipe_Воздух_Земля">
    <rdf:type rdf:resource="#Recipe"/>
    <ex:hasIngredient rdf:resource="#Воздух"/>
    <ex:hasIngredient rdf:resource="#Земля"/>
    <ex:hasResult rdf:resource="#Пыль"/>
    <rdfs:label xml:lang="ru">Воздух + Земля = Пыль</rdfs:label>
  </rdf:Description>

  <!-- Воздух + Огонь = Энергия -->
  <rdf:Description rdf:about="#Recipe_Воздух_Огонь">
    <rdf:type rdf:resource="#Recipe"/>
    <ex:hasIngredient rdf:resource="#Воздух"/>
    <ex:hasIngredient rdf:resource="#Огонь"/>
    <ex:hasResult rdf:resource="#Энергия"/>
    <rdfs:label xml:lang="ru">Воздух + Огонь = Энергия</rdfs:label>
  </rdf:Description>

  <!-- Земля + Вода = Болото -->
  <rdf:Description rdf:about="#Recipe_Земля_Вода">
    <rdf:type rdf:resource="#Recipe"/>
    <ex:hasIngredient rdf:resource="#Земля"/>
    <ex:hasIngredient rdf:resource="#Вода"/>
